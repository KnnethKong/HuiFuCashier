<resources>

    <!-- Base application theme. -->
    <style name="AppTheme" parent="Theme.AppCompat.Light.DarkActionBar">
        <!-- Customize your theme here. -->
        <item name="colorPrimary">@color/colorPrimary</item>
        <item name="colorPrimaryDark">@color/colorPrimaryDark</item>
        <item name="colorAccent">@color/colorAccent</item>
    </style>

    <style name="noTitle" parent="Theme.AppCompat.Light.NoActionBar">
        <item name="colorPrimary">@color/white</item>
        <item name="colorPrimaryDark">@color/transparent</item>
        <item name="android:textColor">@color/white</item>
        <item name="android:textSize">20sp</item>
        <item name="colorAccent">@color/white</item>
    </style>

    <style name="textInputLayoutBG" parent="Theme.AppCompat.Light.NoActionBar">
        <item name="colorPrimary">@color/white</item>
        <item name="colorPrimaryDark">@color/dimgray</item>
        <item name="android:textColor">@color/white</item>
        <item name="android:textSize">20sp</item>
        <item name="colorAccent">@color/white</item>
    </style>

    <style name="AppTheme.NoActionBar">
        <item name="windowActionBar">false</item>
        <item name="windowNoTitle">true</item>
    </style>

    <style name="AppTheme.AppBarOverlay" parent="ThemeOverlay.AppCompat.Dark.ActionBar" />

    <style name="AppTheme.PopupOverlay" parent="ThemeOverlay.AppCompat.Light" />

    <style name="one_fra_btnbg">
        <item name="android:textSize">24sp</item>
        <item name="android:background">@drawable/sk_number_selector</item>
        <item name="android:gravity">center</item>
    </style>

    <style name="one_fra_s1">
        <item name="android:layout_marginBottom">6dp</item>
        <item name="android:layout_marginLeft">6dp</item>
        <item name="android:layout_marginRight">6dp</item>
        <item name="android:layout_marginTop">4dp</item>
        <item name="android:textColor">#ffff</item>
        <item name="android:textSize">18sp</item>

    </style>

    <style name="one_fra_s2">
        <item name="android:layout_marginLeft">6dp</item>
        <item name="android:layout_marginRight">8dp</item>
        <item name="android:layout_marginTop">8dp</item>
        <item name="android:background">@mipmap/sk_bgjiesuan</item>
        <item name="android:gravity">center</item>
        <item name="android:orientation">vertical</item>
    </style>

    <style name="main_fra_s1">
        <item name="android:layout_margin">4dp</item>
        <item name="android:drawablePadding">2dp</item>
        <item name="android:gravity">center_horizontal</item>
        <item name="android:textSize">12sp</item>
    </style>

    <style name="four_fra_s1">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_marginBottom">10dp</item>
        <item name="android:layout_marginLeft">10dp</item>
        <item name="android:layout_marginTop">8dp</item>
        <item name="android:textColor">#362E2B</item>
    </style>

    <style name="four_fra_s2">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">1dp</item>
        <item name="android:layout_marginLeft">12dp</item>
        <item name="android:background">@color/white</item>
    </style>

    <style name="four_fra_s3">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_alignParentRight">true</item>
        <item name="android:layout_centerVertical">true</item>
        <item name="android:layout_marginRight">12dp</item>
        <item name="android:src">@mipmap/backhui</item>
    </style>

    <style name="four_fra_s5">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_marginLeft">6dp</item>
        <item name="android:layout_marginTop">6dp</item>
    </style>

    <style name="four_fra_s4">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textSize">16sp</item>
        <item name="android:layout_marginLeft">12dp</item>
        <item name="android:layout_marginTop">6dp</item>
        <item name="android:textColor">@color/txttwocolor</item>
    </style>
</resources>
